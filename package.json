{
  "name": "ai-interview",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:api": "npm run -w apps/api dev",
    "dev:web": "npm run -w apps/web dev",
    "build": "npm run -w apps/web build && npm run -w apps/api build"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
